{
    "web":{
        "transports": [
            {
                "type": "File",
                "config": {
                    "level": "info",
                    "filename": "./all-logs.log",
                    "handleExceptions": true,
                    "json": true,
                    "maxsize": 5242880,
                    "maxFiles": 5,
                    "colorize": false
                }
            },
            {
                "type": "Console",
                "config": {
                    "level": "all",
                    "handleExceptions": true,
                    "json": false,
                    "colorize": true
                }
            }
        ],
        "exitOnError": false
    }
}